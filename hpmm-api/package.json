{
  "name": "hpmm-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --legacy-watch src/server.ts",
    "db:create": "knex migrate:make --knexfile src/knexfile.ts --migrations-directory db/migrations -x ts $1",
    "db:seed": "knex seed:make $1 --knexfile src/knexfile.ts",
    "db:run": "knex --knexfile src/knexfile.ts migrate:latest",
    "db:rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "db:run:seed": "knex --knexfile src/knexfile.ts seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/request-ip": "^0.0.41",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "node": "^22.16.0",
    "pg": "^8.13.3",
    "request-ip": "^3.3.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/knex": "^0.15.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
