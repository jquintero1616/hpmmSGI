{
  "family": "hpmm-sgi-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::861455913678:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::861455913678:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "backend-sgi",
      "image": "861455913678.dkr.ecr.us-east-2.amazonaws.com/hpmm-sgi-repo:backend-latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "REDIS_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:861455913678:secret:hpmm-sgi/redis/credentials-SZ6Ray:REDIS_URL::"
        },
        {
          "name": "PSQL_DB_USER",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:861455913678:secret:rds!db-c124b581-0b2b-4095-90cd-1a2f03401dd6-6Ls6Nu:username::"
        },
        {
          "name": "PSQL_DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:861455913678:secret:rds!db-c124b581-0b2b-4095-90cd-1a2f03401dd6-6Ls6Nu:password::"
        },
        {
          "name": "SESSION_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:861455913678:secret:hpmm-sgi/session-secret-SZ848a:SESSION_SECRET::"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "CORS_ORIGIN",
          "value": "*"
        },
        {
          "name": "PSQL_DB_HOST",
          "value": "hpmm-sgi-postgres.chyiw8yqwzr6.us-east-2.rds.amazonaws.com"
        },
        {
          "name": "PSQL_DB_PORT",
          "value": "5432"
        },
        {
          "name": "PSQL_DB_NAME",
          "value": "hpmm_sgi"
        },
        {
          "name": "PSQL_DB_DATABASE",
          "value": "hpmm_sgi"
        },
        {
          "name": "PSQL_DB_SCHEMA",
          "value": "mi_esquema"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hpmm-sgi-backend",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "backend",
          "awslogs-create-group": "true"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:3000/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
